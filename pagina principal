function data(){
    var date = new Date();
    var dia = date.getDate();
    var mes = date.getMonth();
    var ano = date.getFullYear();

   

    var data_total = dia + "/" + mes + "/" + ano;
    var data = window.document.getElementById('div_data');
    data.innerHTML = data_total;
}

setInterval(data,500);

